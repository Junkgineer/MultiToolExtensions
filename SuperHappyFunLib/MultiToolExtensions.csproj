<Project Sdk="Microsoft.NET.Sdk">

  <PropertyGroup>
    <TargetFramework>netstandard2.0</TargetFramework>
    <Version>1.0.9</Version>
    <GeneratePackageOnBuild>true</GeneratePackageOnBuild>
    <PackageId>MultiToolExtensions</PackageId>
    <Authors>Jake Meyer</Authors>
    <Product>MultiToolExtensions</Product>
    <Company>Junkgineering</Company>
    <Description>Various handy tools, including: 
- Null checks on multiple types
- SQL URN string parsing and creation
- Basic Levenshtein string calculations
- Class SQL CRUD method generation
- Country, state and county validation and abbreviation for USA, Canada, Mexico, and Great Britain
- SSN string locator</Description>
    <AssemblyVersion>1.0.9.0</AssemblyVersion>
    <FileVersion>1.0.9.0</FileVersion>
    <PackageReleaseNotes>Made change to CleanFileName</PackageReleaseNotes>
    <AssemblyName>MultiToolExtensions</AssemblyName>
    <RootNamespace>MultiToolExtensions</RootNamespace>
  </PropertyGroup>

  <ItemGroup>
    <PackageReference Include="System.Configuration.ConfigurationManager" Version="5.0.0" />
  </ItemGroup>

</Project>
